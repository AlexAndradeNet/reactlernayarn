        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "prettier-plugin-jsdoc": "^0.4.2",
        "react-scripts": "^5.0.1",
